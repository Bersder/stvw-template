  "scripts": {
    "comment": "TODO: 这里需要完善",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --open --config ./build/webpack.dev.client.js",
    "dev:server": "cross-env NODE_ENV=development webpack -w --config build/webpack.dev.server.js --display normal --hide-modules --progress",
